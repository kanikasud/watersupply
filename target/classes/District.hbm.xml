<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
   <class name="in.redexp.watersupply.model.District" table="prwss_main.mst_district" lazy="true">
      <meta attribute="class-description">
         This class contains the district detail. 
      </meta>
      <id name="districtID" type="string" column="district_id">
         <generator class="identity"/>
      </id>
      <property name="districtName" column="district_name" type="string"/>
     
     
    
   </class>
</hibernate-mapping>