<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
   <class name="in.redexp.watersupply.model.SubDivision" table="prwss_main.mst_subdivisional" lazy="true">
      <meta attribute="class-description">
         This class contains the sub-division detail. 
      </meta>
      <id name="subDivisionalID" type="string" column="subdivisional_id">
         <generator class="identity"/>
      </id>
      <property name="subDivisionalName" column="subdivisional_name" type="string"/>
      <property name="divisionalID" column="divisional_id" type="string"/>
      <property name="status" column="status" type="string"/>
    
   </class>
</hibernate-mapping>